<?xml version="1.0" encoding="utf-8"?>
<TableLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="match_parent"
  android:layout_height="match_parent">
    <TableRow android:id="@+id/tableRow2" android:layout_width="wrap_content" android:layout_height="wrap_content">
        <TextView android:text="SSID:" android:id="@+id/textView1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="15dip" android:textColor="#D8D8D8"></TextView>
        <TextView android:text="" android:id="@+id/str_ssid" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textColor="#DF7401"></TextView>
    </TableRow>
    <TableRow android:id="@+id/tableRow1" android:layout_width="wrap_content" android:layout_height="wrap_content">
        <TextView android:text="Security:" android:id="@+id/textView3" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="15dip" android:textColor="#D8D8D8"></TextView>
        <TextView android:text="" android:id="@+id/str_security" android:layout_width="wrap_content" android:layout_height="wrap_content"  android:textColor="#0BDA04"></TextView>
    </TableRow>
    <TableRow android:id="@+id/tableRow3" android:layout_width="wrap_content" android:layout_height="wrap_content">
        <TextView android:text="Power Signal:" android:id="@+id/textView5" android:layout_width="wrap_content" android:layout_height="wrap_content" android:textSize="15dip" android:textColor="#D8D8D8"></TextView>
        <ImageView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/img_signal" android:src="@drawable/icon"></ImageView>
    </TableRow>
    
</TableLayout>
